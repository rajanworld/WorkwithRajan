{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install --include=dev",
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run fix-install && npm run build"
      }
    }
  ]
}
